{
  "includes": [
    "H:\\E-Books"
  ],
  "excludes": [
    "H:\\E-Books\\backup",
    "H:\\E-Books\\duplicates",
    "H:\\E-Books\\excludes"
  ],
  "backupDir": "H:\\E-Books\\backup",
  "duplicateDir": "H:\\E-Books\\duplicates",
  "outputDir": "output",
  "output": "books.json",
  "dataFile": "data.json",
  "timestampFormat": "YYYY-MM-DD_HH-mm-ss",
  "filenameReplacements": [
    "z libray",
    "z.libray",
    "z-libray",
    "zlibrary",
    "libgen li",
    "libgen.li",
    "libgen-li",
    "libgenli",
    "libgen"
  ],
  "tokenization": {
    "enabled": true,
    "minTokenLength": 2,
    "maxTokenLength": 50,
    "removeStopwords": true,
    "useStemming": true,
    "customStopwords": [],
    "fieldsToTokenize": [
      "filename"
    ]
  },
  "index": {
    "compress": false
  }
}
